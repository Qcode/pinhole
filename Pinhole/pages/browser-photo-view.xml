<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatTileView" id="photo_view">
		<property name="classes[]">pinhole-tile-view</property>
		<object class="PinholePhotoTile">
			<object class="SwatImageCellRenderer">
				<property name="image" type="data">image</property>
				<property name="height" type="data">height</property>
				<property name="width" type="data">width</property>
				<property name="occupy_height" type="data">max_height</property>
				<property name="occupy_width" type="data">max_width</property>
				<property name="alt" type="data">title</property>
			</object>
			<object class="SwatTextCellRenderer">
				<property name="text" type="data">title</property>
				<property name="classes[]">photo-title</property>
			</object>
		</object>
	</widget>
	<widget class="SwatPagination" id="pagination">
		<property name="display_parts" type="constant">NEXT|PREV|PAGES</property>
	</widget>
</swatml>
