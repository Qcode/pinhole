<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatMessageDisplay" id="message_display" />
	<widget class="SwatFrame">
		<property name="title" translatable="yes">Upload Photos</property>
		<widget class="PinholePhotoUploader" id="file">
			<property name="action">Photo/Upload</property>
			<property name="target_action">Photo/UploadStatus</property>
		</widget>
		<widget class="AdminNote">
			<property name="content" translatable="yes">Pinhole can process most common image types. Either upload your photos individually, or include multiple photos in a zip archive file.</property>
		</widget>
		<widget class="SwatDisplayableContainer" id="photo_container" />
	</widget>
</swatml>
